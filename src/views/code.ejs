  <script>
    var editor = CodeMirror.fromTextArea(document.getElementById("netcode"), {
      lineNumbers: true,
	  mode: "text/javascript",
	  styleActiveLine: true,
      matchBrackets: true,
      theme: "night",
      extraKeys: {
        "F11": function(cm) {
          cm.setOption("fullScreen", !cm.getOption("fullScreen"));
        },
        "Esc": function(cm) {
          if (cm.getOption("fullScreen")) cm.setOption("fullScreen", false);
        }
      }
    });
  </script>
	

